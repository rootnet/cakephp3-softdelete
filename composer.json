{
    "name": "rootnet/cakephp3-softdelete",
    "description": "SoftDelete plugin for CakePHP, based on pgbi/cakephp3-soft-delete",
    "keywords": ["cakephp", "cakephp 3", "plugin", "soft", "delete", "deletable"],
    "homepage": "https://github.com/rootnet/cakephp3-soft-delete",
    "type": "cakephp-plugin",
    "license": "MIT",
    "support": {
        "source": "https://github.com/rootnet/cakephp3-soft-delete"
    },
    "require": {
        "php": ">=7.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SoftDelete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoftDelete\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    }
}
